{
  "name": "@worldcoin/idkit-server",
  "version": "1.0.0",
  "description": "Server-side IDKit SDK for World ID - RP signing utilities",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "worldcoin",
    "world-id",
    "idkit",
    "identity",
    "proof-of-personhood",
    "zero-knowledge",
    "server"
  ],
  "author": "Tools for Humanity",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/worldcoin/idkit"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@noble/hashes": "^1.7.2",
    "@noble/secp256k1": "^2.2.3"
  },
  "devDependencies": {
    "@types/node": "^20.19.30",
    "@vitest/coverage-v8": "^4.0.18",
    "tsup": "^8.5.1",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
